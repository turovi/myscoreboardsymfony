{% extends 'base.html.twig' %}

{% block title %}User index{% endblock %}

{% block body %}
<ul>
    {% for user in users %}
        {% for player in players %}
            {% if user.player_id == player.id %}
                <li>{{user.pseudo}} = {{player.nickname}}</li>
            
            {% endif %}
        {% endfor %}
    {% endfor %}

</ul>
{% endblock %}
